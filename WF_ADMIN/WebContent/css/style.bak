/* BEGIN bootswatch spacelab */
.navbar-inverse {
  background-color: #446e9b;
  border-color: #345578;
}

.navbar-inverse .navbar-brand {
  color: #dddddd;
}

.navbar-inverse .navbar-brand:hover,
.navbar-inverse .navbar-brand:focus {
  color: #ffffff;
  background-color: transparent;
}

.navbar-inverse .navbar-text {
  color: #dddddd;
}

.navbar-inverse .navbar-nav > li > a {
  color: #dddddd;
}

.navbar-inverse .navbar-nav > li > a:hover,
.navbar-inverse .navbar-nav > li > a:focus {
  color: #ffffff;
  background-color: transparent;
}

.navbar-inverse .navbar-nav > .active > a,
.navbar-inverse .navbar-nav > .active > a:hover,
.navbar-inverse .navbar-nav > .active > a:focus {
  color: #ffffff;
  background-color: transparent;
}

.navbar-inverse .navbar-nav > .disabled > a,
.navbar-inverse .navbar-nav > .disabled > a:hover,
.navbar-inverse .navbar-nav > .disabled > a:focus {
  color: #cccccc;
  background-color: transparent;
}

.navbar-inverse .navbar-toggle {
  border-color: #345578;
}

.navbar-inverse .navbar-toggle:hover,
.navbar-inverse .navbar-toggle:focus {
  background-color: #345578;
}

.navbar-inverse .navbar-toggle .icon-bar {
  background-color: #ffffff;
}

.navbar-inverse .navbar-collapse,
.navbar-inverse .navbar-form {
  border-color: #395c82;
}

.navbar-inverse .navbar-nav > .open > a,
.navbar-inverse .navbar-nav > .open > a:hover,
.navbar-inverse .navbar-nav > .open > a:focus {
  color: #ffffff;
  background-color: transparent;
}

@media (max-width: 767px) {
  .navbar-inverse .navbar-nav .open .dropdown-menu > .dropdown-header {
    border-color: #345578;
  }
  .navbar-inverse .navbar-nav .open .dropdown-menu .divider {
    background-color: #345578;
  }
  .navbar-inverse .navbar-nav .open .dropdown-menu > li > a {
    color: #dddddd;
  }
  .navbar-inverse .navbar-nav .open .dropdown-menu > li > a:hover,
  .navbar-inverse .navbar-nav .open .dropdown-menu > li > a:focus {
    color: #ffffff;
    background-color: transparent;
  }
  .navbar-inverse .navbar-nav .open .dropdown-menu > .active > a,
  .navbar-inverse .navbar-nav .open .dropdown-menu > .active > a:hover,
  .navbar-inverse .navbar-nav .open .dropdown-menu > .active > a:focus {
    color: #ffffff;
    background-color: transparent;
  }
  .navbar-inverse .navbar-nav .open .dropdown-menu > .disabled > a,
  .navbar-inverse .navbar-nav .open .dropdown-menu > .disabled > a:hover,
  .navbar-inverse .navbar-nav .open .dropdown-menu > .disabled > a:focus {
    color: #cccccc;
    background-color: transparent;
  }
}

.navbar-inverse .navbar-link {
  color: #dddddd;
}

.navbar-inverse .navbar-link:hover {
  color: #ffffff;
}
/* END bootswatch spacelab */

.navbar-right .dropdown-menu .list-group {
	margin: 0;
}

.navbar-right .dropdown-menu .heading {
	height: 35px;
	line-height: 35px;
	text-align: center;
}

.navbar-right .dropdown-menu .footer {
	padding: 5px;
	background-color: #efefef;
}

.navbar-right .dropdown-menu .list-group-item {
	border-left: none;
	border-right: none;
}

.navbar-right .dropdown-menu .list-group-item:first-child, .navbar-right .dropdown-menu .list-group-item:last-child {
	border-radius: 0;
}

@media (min-width: 768px) {
	.navbar-right .dropdown-menu {
		right: -1px;
	}
	.navbar-right .dropdown-menu.dropdown-list {
		padding: 0px;
		width: 300px;
	}
}

@media (max-width: 768px) {
	.navbar-collapse {
		max-height: none;
		overflow-x: auto;
	}
	.navbar-right .dropdown-menu.dropdown-list {
		margin-right: 20px;
	}
}

